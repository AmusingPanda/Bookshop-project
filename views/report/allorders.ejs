<h1>Customer Order Listing</h1>
<% if (allrecs.length > 0) {%>

  <table border ='1'>
  <tr>
  <th>Order ID</th>
  <th>Customer ID</th>
  <th>Order Date</th>
  <th>Shipping Method ID</th>
  </tr>
  <% allrecs.forEach((recref, index) => { %>
  <tr>
  <td><%= recref.order_id %></td>
  <td><%= recref.customer_id %></td>
  <td><%= recref.order_date.toDateString() %></td>
  <td><%= recref.shipping_method_id %></td>
  </tr>
  <% }); %>
  </table>
  <% } else { %>
  <p>Not Records Available</p>
  <% } %>

  
