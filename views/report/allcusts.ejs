<h2>Customer Listing</h2>
<% if (allrecs.length > 0) {%>

    <table border ='1'>
    <tr>
    <th>Customer ID</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Username</th>
    </tr>


    <% allrecs.forEach((recref, index) => { %>
    <tr>
    <td><%= recref.customer_id %></td>
    <td><%= recref.first_name %></td>
    <td><%= recref.last_name %></td>
    <td><%= recref.email %></td>
    <td><%= recref.username %></td>
    </tr>
    <% }); %>
    </table>
    <% } else { %>
    <p>No Records Available</p>
    <% } %>
  
  