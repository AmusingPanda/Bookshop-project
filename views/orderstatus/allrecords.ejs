<% if (allrecs.length > 0) {%>

  <table border ='1'>
  <tr>
  <th>Status ID</th>
  <th>Status Name</th>
  <th>Details</th>
  <th>Edit</th>
  <th>Delete</th>
  </tr>
  <% allrecs.forEach((recref, index) => { %>
  <tr>
  <td><%= recref.status_id %></td>
  <td><%= recref.status_name %></td>
  <td><a href='/orderstatus/<%= recref.status_id %>/show'>
  Details</a></td>
  <td><a href='/orderstatus/<%= recref.status_id
  %>/edit'>Edit</a></td>
  <td><a href='/orderstatus/<%= recref.status_id %>/delete'
  onclick="return confirm('Are you sure you want to delete this record?');">Delete</a></td>
  </tr>
  <% }); %>
  </table>
  <% } else { %>
  <p>No Records Available</p>
  <% } %>

  <p> <a href='/orderstatus/addrecord'>Add New</a> </p>
