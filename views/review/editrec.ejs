<h1> Edit Review </h1>
<form action="/review/save" method="post">
<input type="hidden" name="review_id" value="<%= onerec.review_id %>" >
<table border = '1'>
<tr> <td> Customer ID: </td> <td> <input type="number" name="customer_id" value="<%= onerec.customer_id %>" required> </td> </tr>
<tr> <td> Book ID: </td> <td> <input type="number" name="book_id" value="<%= onerec.book_id %>" required> </td> </tr>
<tr> <td> Rating: </td> <td> <input type="number" name="rating" min="1" max="5" value="<%= onerec.rating %>" required> </td> </tr>
<tr> <td> Review Text: </td> <td> <textarea name="review_text" required><%= onerec.review_text %></textarea> </td> </tr>
</table>
<button type="submit">Save</button>
</form>
